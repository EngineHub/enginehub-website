{
    "name": "@enginehub/shared",
    "private": true,
    "description": "Shared libs for the EngineHub sites",
    "version": "1.0.0",
    "author": "EngineHub",
    "main": "./src/index.ts",
    "module": "./src/index.ts",
    "types": "./src/index.ts",
    "type": "module",
    "devDependencies": {
        "@next/eslint-plugin-next": "^14.2.3",
        "@types/node": "^20.12.11",
        "@types/react": "18.3.1",
        "@types/react-dom": "18.3.0",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "eslint": "^8.57.0",
        "eslint-plugin-react": "^7.34.1",
        "eslint-plugin-react-hooks": "^4.6.2",
        "prettier": "^3.2.5",
        "tslib": "^2.6.2",
        "typescript": "^5.4.5"
    },
    "keywords": ["enginehub", "worldedit", "worldguard", "craftbook"],
    "scripts": {
