{
    "name": "@enginehub/shared",
    "private": true,
    "description": "Shared libs for the EngineHub sites",
    "version": "1.0.0",
    "author": "EngineHub",
    "main": "./src/index.ts",
    "module": "./src/index.ts",
    "types": "./src/index.ts",
    "type": "module",
    "devDependencies": {
        "@next/eslint-plugin-next": "^13.4.9",
        "@types/node": "^18.16.19",
        "@types/react": "18.2.14",
        "@types/react-dom": "18.2.6",
        "@typescript-eslint/eslint-plugin": "^5.61.0",
        "@typescript-eslint/parser": "^5.61.0",
        "eslint": "^8.44.0",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "prettier": "^3.0.0",
        "tslib": "^2.6.0",
        "typescript": "^5.1.6"
    },
    "keywords": [
        "enginehub",
        "worldedit",
        "worldguard",
        "craftbook"
    ],
    "scripts": {
        "type-check": "yarn tsc --project ./tsconfig.json --noEmit",
        "lint": "eslint --config ./.eslintrc.js --ext .ts,.tsx src/",
        "lint:fix": "eslint --config ./.eslintrc.js --fix --ext .ts,.tsx src/",
        "prepare": "yarn build",
        "build": "yarn tsc --project ./tsconfig.json",
        "watch": "yarn build --watch"
    },
    "sideEffects": false,
    "dependencies": {
        "next": "^13.4.9",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "styled-components": "^6.0.2"
    }
}
