{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "target": "es5",
        "lib": ["dom", "dom.iterable", "esnext"],
        "skipLibCheck": true,
        "module": "esnext",
        "moduleResolution": "node",
        "isolatedModules": false,
        "noEmit": true,
        "jsx": "react"
    },
    "include": ["src/**/*"],
    "exclude": ["build"]
}
