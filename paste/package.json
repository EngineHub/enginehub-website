{
    "name": "paste",
    "private": true,
    "description": "EngineHub paste site, paste.enginehub.org",
    "version": "1.0.0",
    "author": "EngineHub",
    "type": "module",
    "dependencies": {
        "@enginehub/schematicjs": "^0.9.1",
        "@enginehub/schematicwebviewer": "^4.8.1",
        "@enginehub/shared": "workspace:*",
        "@google-cloud/storage": "^7.3.2",
        "base64-js": "^1.5.1",
        "next": "^13.5.6",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "sharp": "^0.32.6",
        "shortid": "^2.2.16"
    },
    "devDependencies": {
        "@types/node": "^18.18.7",
        "@types/react": "18.2.33",
        "@types/react-dom": "18.2.14",
        "@types/shortid": "^0.0.31",
        "eslint": "^8.52.0",
        "eslint-config-next": "^13.5.6",
        "postcss-preset-env": "^9.2.0",
        "prettier": "^3.0.3",
        "typescript": "^5.2.2"
    },
    "keywords": [
        "pastebin",
        "enginehub",
        "worldedit",
        "worldguard",
        "craftbook"
    ],
    "scripts": {
        "dev": "yarn next",
        "clean": "rm -rf build",
