        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "sharp": "^0.34.3",
        "shortid": "^2.2.17"
    },
    "devDependencies": {
        "@types/node": "^22.16.5",
        "@types/react": "19.1.8",
        "@types/react-dom": "19.1.6",
