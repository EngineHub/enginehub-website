{
    "name": "paste",
    "private": true,
    "description": "EngineHub paste site, paste.enginehub.org",
    "version": "1.0.0",
    "author": "EngineHub",
    "type": "module",
    "dependencies": {
        "@enginehub/schematicjs": "^0.9.2",
        "@enginehub/schematicwebviewer": "^4.9.0",
        "@enginehub/shared": "workspace:*",
        "@google-cloud/storage": "^7.11.2",
        "base64-js": "^1.5.1",
        "next": "^14.2.4",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "sharp": "^0.33.4",
        "shortid": "^2.2.16"
    },
    "devDependencies": {
        "@types/node": "^20.14.2",
        "@types/react": "18.3.3",
        "@types/react-dom": "18.3.0",
        "@types/shortid": "^0.0.32",
        "eslint": "^8.57.0",
        "eslint-config-next": "^14.2.4",
        "postcss-preset-env": "^9.5.14",
        "prettier": "^3.3.2",
        "typescript": "^5.4.5"
    },
    "keywords": [
        "pastebin",
        "enginehub",
        "worldedit",
        "worldguard",
        "craftbook"
    ],
    "scripts": {
        "dev": "yarn next",
        "clean": "rm -rf build",
        "build": "yarn next build",
        "start": "yarn next start -p $PORT",
        "type-check": "yarn tsc --project ./tsconfig.json --noEmit",
        "lint": "yarn next lint",
        "lint:fix": "yarn next lint --fix"
    },
    "sideEffects": ["*.global.css"]
