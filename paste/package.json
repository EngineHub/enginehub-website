{
    "name": "paste",
    "private": true,
    "description": "EngineHub paste site, paste.enginehub.org",
    "version": "1.0.0",
    "author": "EngineHub",
    "type": "module",
    "dependencies": {
        "@enginehub/schematicjs": "^0.9.2",
        "@enginehub/schematicwebviewer": "^4.9.1",
        "@enginehub/shared": "workspace:*",
        "@google-cloud/storage": "^7.15.0",
        "base64-js": "^1.5.1",
        "next": "^15.1.6",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "sharp": "^0.33.5",
        "shortid": "^2.2.17"
    },
    "devDependencies": {
        "@types/node": "^22.10.10",
        "@types/react": "19.0.8",
        "@types/react-dom": "19.0.3",
        "@types/shortid": "^2.2.0",
        "eslint": "^9.19.0",
        "postcss-preset-env": "^10.1.3",
        "prettier": "^3.4.2",
        "typescript": "^5.7.3"
    },
    "keywords": [
        "pastebin",
        "enginehub",
        "worldedit",
        "worldguard",
        "craftbook"
    ],
    "scripts": {
        "dev": "yarn next",
        "clean": "rm -rf build",
