{
    "name": "main-site",
    "private": true,
    "description": "Main EngineHub website, enginehub.org",
    "version": "1.0.0",
    "author": "EngineHub",
    "type": "module",
    "dependencies": {
        "@enginehub/shared": "workspace:*",
        "next": "^16.0.7",
        "react": "^19.2.1",
        "react-dom": "^19.2.1",
        "sharp": "^0.34.5",
        "smooth-scroll": "^16.1.3"
    },
    "devDependencies": {
        "@types/react": "19.2.7",
        "@types/react-dom": "19.2.3",
        "eslint": "^9.39.1",
        "postcss-preset-env": "^10.4.0",
        "prettier": "^3.7.4",
        "typescript": "^5.9.3"
    },
    "keywords": ["gatsby", "enginehub", "worldedit", "worldguard", "craftbook"],
    "scripts": {
        "build": "yarn next build",
        "dev": "yarn next dev",
        "format": "yarn prettier --write src/**/*.{js,jsx}",
        "start": "yarn next start",
        "test": "echo \"Write tests!\"",
        "type-check": "yarn tsc --project ./tsconfig.json --noEmit",
        "lint": "eslint src/ pages/",
        "lint:fix": "yarn lint --fix"
    }
}
