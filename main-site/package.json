{
    "name": "main-site",
    "private": true,
    "description": "Main EngineHub website, enginehub.org",
    "version": "1.0.0",
    "author": "EngineHub",
    "type": "module",
    "dependencies": {
        "@enginehub/shared": "workspace:*",
        "next": "^14.2.5",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "sharp": "^0.33.4",
        "smooth-scroll": "^16.1.3"
    },
    "devDependencies": {
        "@next/eslint-plugin-next": "^14.2.5",
        "@types/react": "18.3.3",
        "@types/react-dom": "18.3.0",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "eslint": "^8.57.0",
        "eslint-plugin-react": "^7.34.3",
        "eslint-plugin-react-hooks": "^4.6.2",
        "postcss-preset-env": "^9.6.0",
        "prettier": "^3.3.2",
        "typescript": "^5.5.3"
    },
    "keywords": ["gatsby", "enginehub", "worldedit", "worldguard", "craftbook"],
    "scripts": {
        "build": "yarn next build",
        "dev": "yarn next dev",
        "format": "yarn prettier --write src/**/*.{js,jsx}",
        "start": "yarn next start",
        "test": "echo \"Write tests!\"",
        "type-check": "yarn tsc --project ./tsconfig.json --noEmit",
        "lint": "yarn next lint",
        "lint:fix": "yarn next lint --fix"
    }
}
