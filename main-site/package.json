{
    "name": "main-site",
    "private": true,
    "description": "Main EngineHub website, enginehub.org",
    "version": "1.0.0",
    "author": "EngineHub",
    "type": "module",
    "dependencies": {
        "@enginehub/shared": "workspace:*",
        "next": "^15.5.2",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "sharp": "^0.34.3",
        "smooth-scroll": "^16.1.3"
    },
    "devDependencies": {
        "@types/react": "19.1.12",
        "@types/react-dom": "19.1.9",
        "eslint": "^9.35.0",
        "postcss-preset-env": "^10.3.1",
        "prettier": "^3.6.2",
        "typescript": "^5.9.2"
    },
    "keywords": [
        "gatsby",
        "enginehub",
        "worldedit",
        "worldguard",
        "craftbook"
    ],
    "scripts": {
        "build": "yarn next build",
        "dev": "yarn next dev",
        "format": "yarn prettier --write src/**/*.{js,jsx}",
        "start": "yarn next start",
        "test": "echo \"Write tests!\"",
        "type-check": "yarn tsc --project ./tsconfig.json --noEmit",
        "lint": "eslint src/ pages/",
        "lint:fix": "yarn lint --fix"
    }
}
